@using App.Domain.Core.HomeService.Dtos
@model IEnumerable<CategoryDto>

<nav>
    @foreach (var menuItem in Model)
    {
        <li class="menu-mega-item-three">
        <a asp-controller="Order" asp-action="ServiceList" asp-route-categoryId="@menuItem.Id" title="@menuItem.Title" class="list-group-item list-group-item-action py-2 ripple">
            @menuItem.Title
        </a>
        </li>
    }
</nav>