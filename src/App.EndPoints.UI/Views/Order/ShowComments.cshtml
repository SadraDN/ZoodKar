@using App.Domain.Core.HomeService.Dtos
@model List<ServiceCommentDto>
<table class="table table-striped table-light">
    <thead>
        <tr>
            <th>کاربر</th>
            <th>نظر ثبت شده</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var comment in Model)
        {
            <tr>
                <td>@comment.CreatedUserId</td>
                <td>@comment.CommentText</td>
            </tr>
        }

        }
    </tbody>
</table>